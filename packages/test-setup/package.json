{
  "name": "@windingtree/org.id-test-setup",
  "version": "1.0.0-beta.46",
  "description": "Ease ORGiD setup for testing purposes",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "build": "rm -rf dist && npx tsc -p tsconfig-build.json",
    "build:docs": "rm -rf docs/generated && npx typedoc --plugin typedoc-plugin-markdown --readme none --out docs/generated src/index.ts",
    "test": "npx hardhat test",
    "test:coverage": "npx nyc --reporter=html --reporter=lcov hardhat test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/windingtree/org.id-sdk.git"
  },
  "keywords": [
    "orgid",
    "windingtree",
    "utilities",
    "typescript"
  ],
  "author": "Winding Tree Developers <dev@windingtree.com>",
  "contributors": [
    "Kostiantyn Smyrnov <kostysh@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/windingtree/org.id-sdk/issues"
  },
  "homepage": "https://github.com/windingtree/org.id-sdk/tree/master/packages/test-setup#readme",
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.4",
    "@windingtree/org.id": "3.0.0-beta.6",
    "@windingtree/org.id-auth": "^1.0.0-beta.46",
    "@windingtree/org.id-test-http-server": "^1.0.0-beta.45",
    "@windingtree/org.id-utils": "^1.0.0-beta.46",
    "@windingtree/org.json-schema": "1.0.2",
    "@windingtree/org.json-utils": "^1.0.0-beta.46",
    "ethers": "5.5.3",
    "hardhat": "2.8.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.4",
    "@types/mocha": "9.0.0",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "axios": "0.24.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "eslint": "8.6.0",
    "nyc": "15.1.0",
    "ts-node": "10.4.0",
    "typedoc": "0.22.10",
    "typedoc-plugin-markdown": "3.11.11",
    "typescript": "4.5.4"
  },
